<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ppstart">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="ProxyPal"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PPSTART">
        <activity
            android:name=".Supporter_Search_Activity"
            android:exported="false" />
        <activity
            android:name=".Business_Profile_Activity"
            android:exported="false" />
        <activity
            android:name=".Login_Activity"
            android:exported="false" />
        <activity
            android:name=".Supporter_Main_Page_Activity"
            android:exported="false" />
        <activity
            android:name=".Create_Owner_Activity"
            android:exported="false" />
        <activity
            android:name=".Create_Supporter_Activity"
            android:exported="false" />
        <activity
            android:name=".Initial_Create_Activity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>